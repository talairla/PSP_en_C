
public class EjecutarHilos {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// Creamos hilos que son subclase de Thread y los ejecutamos
		Hilo1 h1 = new Hilo1();
		Hilo2 h2 = new Hilo2();
		
		h1.start();
		h2.start();
		
		//Lanzar un hilo que implementa runnable
		Hilo3 h3 = new Hilo3();
		
		System.out.println("Fuera del hilo");
	}

}
